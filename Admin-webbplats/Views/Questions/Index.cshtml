@model IEnumerable<Admin_webbplats.Models.Question>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.QuestionText)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Alternative1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Alternative2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Alternative3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Alternative4)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.QuestionText)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Alternative1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Alternative2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Alternative3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Alternative4)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
